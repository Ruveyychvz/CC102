#include <iostream>
using namespace std;

int main()
{
	int sugarPriceUSD, ricePriceUSD, sardinesPriceUSD, coffeePriceUSD, milkPriceUSD;
	int sugarQty, riceQty, sardinesQty, coffeeQty, milkQty;
	int sugarcostUSD, ricecostPound, sardinescostPound, coffeecostUSD, milkcostUSD;
	
	cout << "sugar, rice, sardines, coffee, milk";
	cin >> sugarcostUSD, ricecostPound, sardinescostPound, coffeecostUSD, milkcostUSD;
	
	cout << "sugar cost in USD:";
	cin >> sugarQty;
	cout << "rice cost in Pound:";
	cin >> riceQty;
	cout << "sardines cost in Pound:";
	cin >> sardinesQty;
	cout << "coffee cost in USD:";
	cin >> coffeeQty;
	cout << "milk cost in USD:";
	cin >> milkQty;
	
	
	return 0;
}